{
  "app_settings": {
    "ui": {
      "theme": "dark",
      "language": "en-US",
      "font_size": 14,
      "show_timestamps": true
    },
    "notifications": {
      "enable_desktop_notifications": true,
      "mute_all_notifications": false,
      "do_not_disturb": {
        "enabled": false,
        "start_time": "22:00",
        "end_time": "07:00"
      }
    },
    "connectivity": {
      "auto_reconnect": true,
      "reconnect_attempts": 5,
      "reconnect_interval_seconds": 10
    },
    "voice_chat": {
      "default_microphone": "system_default",
      "default_speaker": "system_default",
      "push_to_talk_enabled": false,
      "push_to_talk_key": "Space"
    },
    "premium": {
      "custom_css_background_enabled": true,
      "custom_css_background_url": "https://cdn.krynet.com/user-backgrounds/4zx16-bg.css"
    },
    "theme_overrides": {
      "enabled": true,
      "context": "server_view",
      "use_boosted_server_theme": true,
      "revert_on_exit": true,
      "conditions": {
        "must_be_boosted": true,
        "must_have_custom_css": true
      },
      "fallbacks": {
        "free_users": "dark",
        "premium_users": "custom_css_background_url"
      }
    }
  }
}
